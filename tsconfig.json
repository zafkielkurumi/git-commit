{
  "compilerOptions": {
      "target": "ES5",
      "module": "CommonJS",
      "outDir": "./bin",
      "sourceMap": false,
      "watch": false,
      "esModuleInterop": true,
      "noEmitOnError": true,
      "allowJs": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "lib": ["esnext",]
  },
  "exclude": [
      "node_modules",
      "bin"
  ],
}